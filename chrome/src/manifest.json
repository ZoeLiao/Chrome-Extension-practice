{
    "name": "Personal Chrome",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}