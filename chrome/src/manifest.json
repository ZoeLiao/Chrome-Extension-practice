{
    "name": "Practice Chrome Extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"
        }
    ],
    "manifest_version": 2
}
